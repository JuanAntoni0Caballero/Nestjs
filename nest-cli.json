{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src", 
  "compilerOptions": {
    "deleteOutDir": true
  },
  "projects": {
    "default": {
      "type": "application",
      "root": "src",
      "entryFile": "main",
      "sourceRoot": "src"
    },
    "prisma": {
      "type": "application",
      "root": "srcPrisma",
      "entryFile": "main",
      "sourceRoot": "srcPrisma"
    }
  }
}
